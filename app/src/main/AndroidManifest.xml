<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Appli_gest_vannes"
        tools:targetApi="31">
        <activity
            android:name=".AjoutReleveActivity"
            android:label="@string/ajoutReleveLabel"
            android:parentActivityName=".ListeCommunes"
            android:exported="false" />
        <activity
            android:name=".ReleveActivity"
            android:exported="false"
            android:label="@string/releve"
            android:parentActivityName=".ListeCommunes"
            android:theme="@style/Theme.EmptyView" />
        <activity
            android:name=".ListeVannes"
            android:exported="false"
            android:label="@string/listeVannes"
            android:parentActivityName=".ListeCommunes"
            android:theme="@style/Theme.EmptyView" />
        <activity
            android:name=".ListeSecteurs"
            android:exported="false"
            android:label="@string/listeSecteurs"
            android:parentActivityName=".ListeCommunes"
            android:theme="@style/Theme.EmptyView" />
        <activity
            android:name=".ListeCommunes"
            android:exported="false"
            android:label="@string/listeCommunes"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.EmptyView" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Appli_gest_vannes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>